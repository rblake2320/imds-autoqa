<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="IMDS AutoQA Healing Integration Suite" verbose="1" parallel="none">

  <listeners>
    <listener class-name="autoqa.reporting.AllureListener"/>
  </listeners>

  <test name="Healing Integration Tests">
    <packages>
      <package name="autoqa.integration"/>
    </packages>
    <groups>
      <run>
        <include name="healing"/>
      </run>
    </groups>
  </test>

</suite>
